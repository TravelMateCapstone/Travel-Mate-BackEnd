    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=TravelMatev1;Trusted_Connection=True;TrustServerCertificate=True"
