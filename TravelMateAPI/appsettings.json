{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyHere123456", // Khoá bí mật để mã hóa JWT
    "Issuer": "YourIssuer", // Nhà phát hành JWT
    "Audience": "YourAudience", // Đối tượng người nhận JWT
    "DurationInMinutes": 60
  },
  "runtimeOptions": {
    "configProperties": {
      "System.Globalization.Invariant": false
    }
  }
}
